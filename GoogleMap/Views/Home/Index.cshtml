<!DOCTYPE html>
<html>
<head>
    <style>
        #map {
            height: 400px;
            width: 100%;
        }
    </style>
</head>
<body>
    <h3>OnurKAPICI</h3>
    <div id="map"></div>
    <script>
        function initMap() {
            var uluru = { lat: 40.9449108, lng: 29.2909245 };
            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 10,
                center: uluru
            });
            var marker = new google.maps.Marker({
                position: uluru,
                map: map
                
            });

        }

    </script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD0bL34wg4_96sYq5ZoHAXCUijemaOP6lI&callback=initMap">
    </script>
</body>
</html>


